// Como podemos melhorar o esse c√≥digo usando TS? 
enum{
    atriz,
    Padeiro
}
interface pessoa {nome:string,idade:number,profissao:enum}

let pessoa1:pessoa = {
    nome : "maria",
    idade : 29,
    profissao:atriz
};


let pessoa2 :pessoa {
    nome : "roberto",
    idade : 19,
    profissao:Padeiro
}


let pessoa3:pessoa {
    nome: "laura",
    idade: "32",
    profissao:atriz
};

let pessoa4 :pessoa  {
    nome = "carlos",
    idade = 19,
    profissao = Padeiro
}